<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testviewpagerandtabhost"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Yun.TextView"
         >
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".normalsettings"></activity>
        <activity android:name=".allsettings"></activity>
        <activity android:name=".yunsettings"></activity>
    </application>

</manifest>